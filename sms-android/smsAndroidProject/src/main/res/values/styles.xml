<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <!-- customize the color palette -->
        <item name="colorPrimary">@color/theme_primary</item>
        <item name="colorPrimaryDark">@color/theme_primary_dark</item>
        <item name="colorAccent">@color/theme_accent</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:displayOptions">showHome|homeAsUp|showTitle</item>
        <item name="android:icon">@android:color/transparent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="preferenceTheme">@style/PreferenceThemeOverlay.v14.Material</item>
        <item name="android:activatedBackgroundIndicator">?android:attr/selectableItemBackground</item> <!-- for preference background issue -->
    </style>

    
    <!-- Dashboard style -->
    <style name = "dashboard_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>
    
     <style name = "dashboard_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/dimen_1080_20sp</item>
        <item name="android:textColor">@android:color/white</item>
     </style>
     <!-- End Dashboard style -->
     
     <!-- RoutePlanner style -->
    
     <style name = "routepaln_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/dimen_1080_15sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name ="android:layout_marginLeft">@dimen/dimen_1080_15dp</item>
        <item name ="android:layout_marginTop">@dimen/dimen_1080_5dp</item>
        <item name = "android:background">@color/route_card_back</item>
       </style>
     
      <style name = "routepaln_tv_tag" parent="routepaln_tv">
        <item name = "android:textStyle">bold</item>
      </style>
      <!-- End RoutePlanner style -->
      
      
      <!-- Network setting style -->
      <style name = "network_ll_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/dimen_1080_7dp</item>
        <item name="android:paddingBottom">@dimen/dimen_1080_7dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/dimen_1080_20dp</item>
      </style>
    
     <style name = "network_tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textSize">@dimen/dimen_1080_20sp</item>
        <item name="android:textColor">@color/net_text</item>
        <item name="android:layout_weight">2</item>
       </style>
     
      <style name = "network_tv_tag" parent="network_tv">
        <item name = "android:textStyle">bold</item>
        <item name="android:textColor">@color/net_tag</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginRight">@dimen/dimen_1080_45dp</item>
      </style>
      <!-- End Network setting style -->
      
      <!-- Charging actions style -->
        
        <style name = "charge_action_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_1080_40dp</item>
        <item name="android:textColor">@color/transparent_black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/dimen_1080_13sp</item>
       </style>
        
     <!-- End Charging actions style -->
      
       <style name="FabStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/spacing_large</item>
        <item name="android:src">@drawable/xiangji</item>
        <item name="borderWidth">0dp</item>
        <item name="elevation">6dp</item>
        <item name="pressedTranslationZ">12dp</item>
        <item name="rippleColor">@android:color/white</item>
    </style>

    <style name="AppCompatTheme" parent="@style/Theme.AppCompat.Light">
        <item name="android:windowActionBar">false</item>
    </style>

    <style name="MyMaterialTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">true</item>
    </style>
